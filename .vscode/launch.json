    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${fileDirname}",
            "args": []
        },
        {
            "name": "Python Debugger: Streamlit",
            "type": "debugpy",
            "request": "launch",
            "program": "/home/itcen/miniconda3/envs/cenai/bin/streamlit",
            "args": [
                "run",
                "${file}"
            ],
            "console": "integratedTerminal",
            "cwd": "${fileDirname}",
        }
    ]
}